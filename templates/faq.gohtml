{{define "page"}}
<div class=" px-6">
    <h1 class="py-4 text-4xl semibold tracking-loose">FAQ Page</h1>
    <ul class="grid grid-cols-2 gap-16">
        {{range .}}
            {{template "qa" .}}
        {{end}}
    </ul>


</div>
{{end}}

{{define "qa"}}
    <li class="bg-slate-300 border-t border-slate-700 py-1 px-2">
    <span class="block text-lg text-grey-800">{{.Question}}</span>
    <span class="block text-sm text-grey-500">{{.Answer}}</span>
    </li>
{{end}}